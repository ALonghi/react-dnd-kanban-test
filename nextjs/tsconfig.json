{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "paths": {
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/model/*": [
        "src/model/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/components/*": [
        "src/components/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "."
  ],
  "exclude": [
    "node_modules"
  ]
}
